@model IEnumerable<WebApplication.Conference>
@*@model IEnumerable<WebApplication.Models.Class>*@
@{
    ViewBag.Title = "Index";
}

<h2> Your conference(s) as a chair</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.conference_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.start_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.end_date)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {

        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.conference_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.start_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.end_date)
            </td>

            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id_conference }) |
                @Html.ActionLink("Details", "Details", new { id = item.id_conference }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id_conference })
            </td>
        </tr>
    }

</table>
